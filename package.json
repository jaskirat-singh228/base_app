{
	"name": "base_app",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"android": "react-native run-android --list-devices",
		"ios": "react-native run-ios --list-devices",
		"lint": "eslint .",
		"start": "react-native start --reset-cache --client-logs",
		"test": "jest",
		"pods": "cd ios && RCT_NEW_ARCH_ENABLED=1 bundle install && bundle exec pod install && cd ..",
		"cleanAndroid": "cd android && ./gradlew clean && cd ..",
		"cleanProject": "npx react-native clean && npm run deleteYarnlock && npm run deletePodlock && npm run deleteGradleFiles",
		"cleanPods": "cd ios && pod deintegrate && rm Podfile.lock && cd .."
	},
	"dependencies": {
		"@react-native-async-storage/async-storage": "^2.2.0",
		"@react-native-vector-icons/material-design-icons": "^12.3.0",
		"@react-navigation/bottom-tabs": "^7.4.7",
		"@react-navigation/native": "^7.1.17",
		"@react-navigation/native-stack": "^7.3.26",
		"@reduxjs/toolkit": "^2.8.2",
		"react": "19.1.0",
		"react-native": "0.81.1",
		"react-native-safe-area-context": "^5.6.1",
		"react-native-screens": "^4.15.4",
		"react-redux": "^9.2.0"
	},
	"devDependencies": {
		"@babel/core": "^7.25.2",
		"@babel/preset-env": "^7.25.3",
		"@babel/runtime": "^7.25.0",
		"@react-native-community/cli": "20.0.0",
		"@react-native-community/cli-platform-android": "20.0.0",
		"@react-native-community/cli-platform-ios": "20.0.0",
		"@react-native/babel-preset": "0.81.1",
		"@react-native/eslint-config": "0.81.1",
		"@react-native/metro-config": "0.81.1",
		"@react-native/typescript-config": "0.81.1",
		"@types/jest": "^29.5.13",
		"@types/react": "^19.1.0",
		"@types/react-test-renderer": "^19.1.0",
		"babel-plugin-module-resolver": "^5.0.2",
		"eslint": "^8.19.0",
		"jest": "^29.6.3",
		"prettier": "2.8.8",
		"react-test-renderer": "19.1.0",
		"typescript": "^5.8.3"
	},
	"engines": {
		"node": ">=20"
	}
}
